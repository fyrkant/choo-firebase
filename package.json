{
  "name": "choo-firebase",
  "version": "0.0.1",
  "description": "Example of using firebase with choo.",
  "main": "index.js",
  "scripts": {
    "deploy": "npm run compile && firebase deploy",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint 'src/**/*.js' --fix",
    "start": "npm run copy && budo src/app.js --open --dir=dist --live --pushstate -- ",
    "copy": "mkdir -p dist && cp src/index.html dist/",
    "compile": "npm run copy && browserify src/app.js | uglifyjs -c > dist/app.js "
  },
  "browserify": {
    "transform": [
      "sheetify/transform",
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mw222rs/choo-firebase.git"
  },
  "author": "Mattias W",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mw222rs/choo-firebase/issues"
  },
  "homepage": "https://github.com/mw222rs/choo-firebase#readme",
  "dependencies": {
    "choo": "^3.0.0",
    "firebase": "^3.6.5",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "budo": "^9.4.5",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "insert-css": "^2.0.0",
    "sheetify": "^6.0.1",
    "uglify-js": "^2.7.5"
  }
}
